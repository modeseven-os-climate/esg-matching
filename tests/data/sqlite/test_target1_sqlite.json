{
  "file_processing": {
    "file_path": "../../../tests/data/test_target1.csv",
    "file_extension_pattern": "*.csv",
    "filename_pattern": "test_target1",
    "encoding": "utf-8",
    "separator": ","
  },
  "data_source": {
    "name": "DS_TGT1",
    "table_name": "ESG_MATCH_TGT1",
    "create_table": "True",
    "if_table_exists": "drop",
    "primary_keys": ["UNIQUE_ID"],
    "attributes": {
      "UNIQUE_ID":  ["UNIQUE_ID","int"],
      "ISIN":  ["ISIN", "str"],
      "LEI":  ["LEI", "str"],
      "NAME":  ["COMPANY", "str"],
      "COUNTRY_CODE": ["COUNTRY", "str"]
    },
    "matching_role": "target",
    "matching_alias": {
      "ISIN":  "ISIN",
      "LEI":  "LEI",
      "COMPANY": "COMPANY",
      "COUNTRY": "COUNTRY"
    },
    "map_to_matching": {
      "TGT_ID":  "UNIQUE_ID",
      "ISIN":  "ISIN",
      "LEI":  "LEI",
      "TGT_COMPANY": "COMPANY",
      "TGT_COUNTRY": "COUNTRY"
    }
  },
  "matching_policy": {
    "matching_with_ref1": {
      "dfm": {
        "isin":  ["ISIN"]
      } ,
      "drm": {
        "name+country":  ["COMPANY", "COUNTRY"]
      },
      "ifm": {
        "isin":  ["ISIN"],
        "name+country":  ["COMPANY", "COUNTRY"]
      }
    }
  }
}
